{
  "name": "Nugg's Strain Finder",
  "description": "Let Nugg, your virtual budtender, know what you're looking for from your ideal strain!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Find your ideal strain"
  }, 
  "permissions": ["contextMenus", "tabs", "storage"]
}

